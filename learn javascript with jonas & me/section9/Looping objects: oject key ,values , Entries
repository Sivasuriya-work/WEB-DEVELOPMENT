In ES2020 recent update  



// Object keys
const property = Object.keys(days);
console.log(days);
console.log(Object.keys(days));
for (const day of Object.keys(days)) {
  console.log(day);
}

// Object values

console.log(Object.values(days));
for (const day of Object.values(days)) {
  console.log(day);
}

//Object Entries
console.log(Object.entries(openingHours));
const entry = Object.entries(openingHours);
for (const [key, val] of entry) {
  console.log(key, val);
}
for (const [key, { open, close }] of entry) {
  console.log(key, open, close);
}



