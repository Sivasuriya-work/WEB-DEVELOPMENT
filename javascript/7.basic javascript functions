var age = 18
function voterAge(){ // passing args
	if(age>=18){
		console.log("Eligible to vote")
	}
}

function voterAge(voterage){ // passing args
	if(age>=18){
		console.log("Eligible to vote")
	}
}
voterAge() // without args
voterAge(age) // with args
// console.log(voterage) // local variable in function not accessable // voterage is not defined

